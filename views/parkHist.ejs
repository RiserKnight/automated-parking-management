<%- include('partials/navbar') %>
  <h1>Parking History</h1>
  <div class="table-responsive">
    <%if(users[0]){%>
    <table class="table table-bordered table-hover">
        <thead id="stockNameRow">
            <%const arrH = Object.keys(users[0]);%>
            <tr>
                <th>
                    Serial
                </th>
                <%arrH.forEach((head)=>{%>
                <th style="color: rgb(95, 98, 95);">
                    <h4><%=head%></h4>
                </th>
                <%})%>
            </tr>
        </thead>

        <tbody>
        <%var a=1%>
        <%users.forEach((user)=>{%>
            <%const arr = Object.values(user); %>
            
            <tr>
                <td>
                    <%=a%>
                </td>
                <%arr.forEach((data)=>{%>
                <td>
                    <h5> <b><%=data%></b></h5>
                </td>
                <%})%>
            </tr>
         <%a++})%>
        </tbody>
    </table>
    <%}else{%>
        <h1>Empty</h1>
    <%}%>
</div>
<div style="float: right; margin-right: 3rem; font-size: 1.2rem;">
    <label for="due">Total Due Amount: <b><%=dueAmount%></b></label>
</div>


<style>
    .table {
        width: 100%;
        align-items: center;
        text-align: center;
        padding: 3rem;
    }
    td, th {
        border: 1px solid black;
    }
    th {
        background-color: rgb(219, 219, 219);
        font-weight: bold;
    }
    th h4 {
        color: black;
    }
</style>
<%- include('partials/footer') %>
